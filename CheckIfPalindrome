// Check whether a given no. is a palindrome or not. e.g. 12321. 
#include <stdio.h>

int main() {
    int a,digit, rem, b[100], isPalindrome = 1, i=0;
    printf("Enter no:");
    scanf("%d", &a);
    digit=a;
    rem=a;
    while(digit!=0) {
        rem=digit%10;
        b[i]=rem;
        digit=digit/10;
        i++;
    }
    for(int j=0; j<i; j++) {
        if(b[j]!=b[i-j-1]) {
            isPalindrome=0;
            break; 
        }  
    }
    if(isPalindrome==1)
        printf("The no is palindrome\n");
    else
        printf("The no is not palindrome\n");
    return 0;
}
